    "backfill:adevent:prod": "npx @firebaseextensions/fs-bq-import-collection --non-interactive --project=lootbox-fund-prod --dataset=firestore_export --source-collection-path=ad-event  --table-name-prefix=ad_event --query-collection-group=false --dataset-location=asia-southeast1",
