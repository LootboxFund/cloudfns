    "backfill:lootbox:prod": "npx @firebaseextensions/fs-bq-import-collection --non-interactive --project=lootbox-fund-prod --dataset=firestore_export --source-collection-path=lootbox  --table-name-prefix=lootbox --query-collection-group=false --dataset-location=asia-southeast1",
    "backfill:claim:prod": "npx @firebaseextensions/fs-bq-import-collection --non-interactive --project=lootbox-fund-prod --dataset=firestore_export --source-collection-path=claim  --table-name-prefix=claim --query-collection-group=true --dataset-location=asia-southeast1",
