    "deploy:schema:claim:staging": "npx @firebaseextensions/fs-bq-schema-views --non-interactive --project=lootbox-fund-staging --dataset=firestore_export  --table-name-prefix=claim --schema-files=./src/schema/claim_schema.json",
    "deploy:schema:claimprivacy:staging": "npx @firebaseextensions/fs-bq-schema-views --non-interactive --project=lootbox-fund-staging --dataset=firestore_export  --table-name-prefix=claim --schema-files=./src/schema/claim_privacy_scope_schema.json",
